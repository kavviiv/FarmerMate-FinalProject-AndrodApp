<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FAF5F5"
    android:fillViewport="true"
    tools:context=".HomePage">


    <View

        android:id="@+id/tablepage"
        android:layout_width="402dp"
        android:layout_height="0dp"
        android:layout_marginTop="144dp"
        android:background="#04E3FF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.95" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="146dp"
        android:layout_height="110dp"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="44dp"
        app:layout_constraintStart_toStartOf="@+id/tablepage"
        app:layout_constraintTop_toTopOf="@+id/tablepage"
        app:srcCompat="@drawable/calendar" />

    <View
        android:id="@+id/weatherpage"
        android:layout_width="190dp"
        android:layout_height="190dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHeight_percent="0.25"
        app:layout_constraintTop_toBottomOf="@+id/tablepage"
        app:layout_constraintWidth_percent="0.95" />

    <View
        android:id="@+id/recompage"
        android:layout_width="190dp"
        android:layout_height="190dp"
        android:layout_marginStart="12dp"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="20dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/weatherpage"
        app:layout_constraintHeight_percent="0.25"
        app:layout_constraintHorizontal_bias="0.166"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tablepage"
        app:layout_constraintWidth_percent="0.95" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="144dp"
        android:layout_height="37dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/tkodb"
        android:text="@string/redd"
        android:textColor="#000000"
        android:textSize="24sp"
        app:autoSizeTextType="uniform"
        app:layout_constraintEnd_toStartOf="@+id/recompage"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toEndOf="@+id/recompage"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/tabb"
        android:layout_width="210dp"
        android:layout_height="60dp"
        android:layout_marginStart="160dp"
        android:layout_marginLeft="160dp"
        android:fontFamily="@font/tkodb"
        android:text="@string/tab"
        android:textColor="#000000"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toBottomOf="@+id/tablepage"
        app:layout_constraintStart_toStartOf="@+id/tablepage"
        app:layout_constraintTop_toTopOf="@+id/tablepage"
        app:layout_constraintVertical_bias="0.534" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="144dp"
        android:layout_height="44dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:fontFamily="@font/tkodb"
        android:text="@string/weath"
        android:textColor="#000000"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toBottomOf="@+id/weatherpage"
        app:layout_constraintStart_toStartOf="@+id/weatherpage"
        app:layout_constraintTop_toTopOf="@+id/weatherpage"
        app:layout_constraintVertical_bias="0.923" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="114dp"
        android:layout_height="109dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toStartOf="@+id/recompage"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/recompage"
        app:layout_constraintTop_toTopOf="@+id/recompage"
        app:srcCompat="@drawable/rec" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="126dp"
        android:layout_height="133dp"
        app:layout_constraintEnd_toStartOf="@+id/weatherpage"
        app:layout_constraintStart_toEndOf="@+id/weatherpage"
        app:layout_constraintTop_toTopOf="@+id/weatherpage"
        app:srcCompat="@drawable/icon_02d" />

    <view
        android:id="@+id/nearlocate"
        class="androidx.appcompat.widget.ActionMenuView"
        android:layout_width="190dp"
        android:layout_height="190dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="28dp"
        android:background="#FFFFFF"
        app:backgroundTint="#FFFCFC"
        app:boxBackgroundColor="#FFFFFF"
        app:cardBackgroundColor="@color/login_form_details"
        app:layout_constraintEnd_toStartOf="@+id/view2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/recompage" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="81dp"
        android:layout_height="87dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toStartOf="@+id/nearlocate"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/nearlocate"
        app:layout_constraintTop_toTopOf="@+id/nearlocate"
        app:srcCompat="@drawable/googlemaps" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/tkodb"
        android:text="@string/nlo"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/nearlocate"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toEndOf="@+id/nearlocate"
        app:layout_constraintTop_toBottomOf="@+id/imageView6" />

    <view
        android:id="@+id/view2"
        class="androidx.appcompat.widget.ActionMenuView"
        android:layout_width="190dp"
        android:layout_height="190dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weatherpage"
        tools:visibility="gone" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="109dp"
        android:layout_height="90dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toStartOf="@+id/view2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2"
        app:srcCompat="@drawable/payday"
        tools:visibility="gone" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/tkodb"
        android:text="@string/expmoney"
        android:textColor="#000000"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/view2"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        tools:visibility="gone" />

    <ImageView
        android:id="@+id/profile"
        android:layout_width="83dp"
        android:layout_height="71dp"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/users" />


</androidx.constraintlayout.widget.ConstraintLayout>