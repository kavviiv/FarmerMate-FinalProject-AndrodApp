<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <View
        android:id="@+id/view6"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="#00FFD9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view7"
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:layout_marginTop="52dp"
        android:background="#87FFE7"
        app:layout_constraintTop_toTopOf="@+id/calendarView"
        tools:layout_editor_absoluteX="0dp" />

    <CalendarView
        android:id="@+id/calendarView"
        android:layout_width="434dp"
        android:layout_height="402dp"
        android:layout_marginTop="16dp"
        android:background="#0045149E"
        android:dateTextAppearance="@style/TextAppearance.AppCompat.Small"
        android:minDate="1/01/2019"
        android:selectedWeekBackgroundColor="#FFFFFF"
        android:unfocusedMonthDateColor="#E80C0C"
        android:weekDayTextAppearance="@style/TextAppearance.AppCompat.Medium"
        android:weekSeparatorLineColor="#FFFEFE"
        app:layout_constraintEnd_toStartOf="@+id/view6"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toEndOf="@+id/view6"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>